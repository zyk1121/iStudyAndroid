        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="DD279b2a90afdf0ae7a3796787a0742e" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FCUI.FCUIActivity" />
        <activity android:name=".FCUI.FCUIListViewActivity" />
        <activity android:name=".FCActivity.FCActivityActivity" />
        <activity android:name=".FCActivity.FCActivityLifeCycle">
